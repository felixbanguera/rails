<ul>
	<% if user_signed_in? %>
		<li><%= link_to "Home", root_path %></li>
		<li><%= link_to current_user.email, session_path(current_user) %></li>
		<li><%= link_to "Profile", profiles_path %></li>
		<li><%= link_to "Publish Profile", new_profile_path %></li>
		<li><%= link_to "Log out", session_path(current_user), method: :delete %></li>
	<% else %>
		<li><%= link_to "Home", root_path %></li>
		
		<li><%= link_to "Log in", new_session_path %></li>
		<li><%= link_to "Sign up", new_user_path %></li>
	<% end -%>
</ul>